{
  "name": "@milkshakeio/webidl2ts",
  "version": "1.0.7",
  "description": "Converts Web IDL to Typescript (.d.ts)",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && chmod 755 dist/cli.js",
    "lint": "tsc --noEmit",
    "start": "yarn build && ./dist/cli.js",
    "format": "yarn prettier --write .",
    "test": "yarn start -e -d -n Example -i example/test.idl -o example/test.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giniedp/webidl2ts.git"
  },
  "homepage": "https://github.com/giniedp/webidl2ts",
  "author": "Alexander Gr√§fenstein",
  "license": "MIT",
  "files": [
    "dist",
    "Readme.md",
    "package.json",
    "LICENSE.md"
  ],
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/webidl2": "^23.13.5",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0"
  },
  "dependencies": {
    "jsdom": "^19.0.0",
    "typescript": "^4.0.5",
    "webidl2": "^24.1.1",
    "yargs": "^16.1.0"
  },
  "bin": {
    "webidl2ts": "./dist/cli.js"
  }
}
